CREATE TABLE BOARD 
(
  B_CORD NUMBER NOT NULL 
, COMMENT_CORD NUMBER NOT NULL 
, B_NUM NUMBER NOT NULL 
, USER_NICK VARCHAR2(30 BYTE) NOT NULL 
, ADMIN_PERM VARCHAR2(5 BYTE) NOT NULL 
, B_DATE VARCHAR2(10 BYTE) NOT NULL 
, TITLE VARCHAR2(50 BYTE) NOT NULL 
, CONTENT VARCHAR2(500 BYTE) NOT NULL 
, USER_ID VARCHAR2(20 BYTE) NOT NULL 
, CONSTRAINT BOARD_PK PRIMARY KEY 
  (
    COMMENT_CORD 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX BOARD_PK ON BOARD (COMMENT_CORD ASC) 
      LOGGING 
      TABLESPACE SYSAUX 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE SYSAUX 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;

ALTER TABLE BOARD
ADD CONSTRAINT BOARD_FK1 FOREIGN KEY
(
  USER_ID 
)
REFERENCES MEMBERS
(
  USER_ID 
)
ENABLE;

ALTER TABLE BOARD
ADD CONSTRAINT BOARD_FK2 FOREIGN KEY
(
  B_CORD 
)
REFERENCES BOARD_LIST
(
  B_CORD 
)
ENABLE;
----
CREATE TABLE COMMENT_LIST 
(
  B_CORD NUMBER NOT NULL 
, B_NUM NUMBER NOT NULL 
, COMMENT_CORD NUMBER NOT NULL 
, USER_NICK VARCHAR2(30 BYTE) NOT NULL 
, ADMIN_PERM VARCHAR2(10 BYTE) NOT NULL 
, COMMENT_DATE DATE NOT NULL 
, COMMENT_CONTENT VARCHAR2(50 BYTE) NOT NULL 
, USER_ID VARCHAR2(20 BYTE) NOT NULL 
) 
LOGGING 
TABLESPACE SYSAUX 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;

ALTER TABLE COMMENT_LIST
ADD CONSTRAINT COMMENT_LIST_FK1 FOREIGN KEY
(
  USER_ID 
)
REFERENCES MEMBERS
(
  USER_ID 
)
ENABLE;

ALTER TABLE COMMENT_LIST
ADD CONSTRAINT COMMENT_LIST_FK2 FOREIGN KEY
(
  COMMENT_CORD 
)
REFERENCES BOARD
(
  COMMENT_CORD 
)
ENABLE;
