CREATE TABLE MEMBERS 
(
  USER_ID VARCHAR2(20 BYTE) NOT NULL 
, USER_NICK VARCHAR2(30 BYTE) NOT NULL 
, USER_PW VARCHAR2(20 BYTE) NOT NULL 
, USER_NAME VARCHAR2(20 BYTE) NOT NULL 
, USER_PERMISSION VARCHAR2(20 BYTE) NOT NULL 
, USER_EMAIL VARCHAR2(20 BYTE) 
, USER_PHONE VARCHAR2(20 BYTE) 
, CONSTRAINT MEMBERS_PK PRIMARY KEY 
  (
    USER_ID 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX MEMBERS_PK ON MEMBERS (USER_ID ASC) 
      LOGGING 
      TABLESPACE SYSAUX 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        INITIAL 65536 
        NEXT 1048576 
        MINEXTENTS 1 
        MAXEXTENTS UNLIMITED 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE SYSAUX 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  INITIAL 65536 
  NEXT 1048576 
  MINEXTENTS 1 
  MAXEXTENTS UNLIMITED 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;
----
CREATE TABLE CAR 
(
  CAR_NUM VARCHAR2(11 BYTE) NOT NULL 
, MANUFAC VARCHAR2(20 BYTE) NOT NULL 
, CAR_MODEL VARCHAR2(50 BYTE) NOT NULL 
, CAR_YEAR NUMBER NOT NULL 
, CAR_DISTANCE NUMBER NOT NULL 
, ACCIDENT_CHEACK VARCHAR2(9 BYTE) NOT NULL 
, CAR_KIND VARCHAR2(20 BYTE) NOT NULL 
, FUEL VARCHAR2(10 BYTE) NOT NULL 
, PRICE NUMBER NOT NULL 
, DEALER_ID VARCHAR2(30 BYTE) NOT NULL 
, ADMIN_PERM VARCHAR2(5 BYTE) 
, UPLOAD_DATE DATE 
, PIC1 VARCHAR2(100 BYTE) 
, PIC2 VARCHAR2(100 BYTE) 
, PIC3 VARCHAR2(100 BYTE) 
, PIC4 VARCHAR2(100 BYTE) 
, PIC5 VARCHAR2(100 BYTE) 
, PIC6 VARCHAR2(100 BYTE) 
, PIC7 VARCHAR2(100 BYTE) 
, PIC8 VARCHAR2(100 BYTE) 
, PIC9 VARCHAR2(100 BYTE) 
, PIC10 VARCHAR2(100 BYTE) 
, PIC11 VARCHAR2(100 BYTE) 
, PIC12 VARCHAR2(100 BYTE) 
, PIC13 VARCHAR2(100 BYTE) 
, PIC14 VARCHAR2(100 BYTE) 
, PIC15 VARCHAR2(100 BYTE) 
, PIC16 VARCHAR2(100 BYTE) 
, PIC17 VARCHAR2(100 BYTE) 
, PIC18 VARCHAR2(100 BYTE) 
, PIC19 VARCHAR2(100 BYTE) 
, PIC20 VARCHAR2(100 BYTE) 
, PIC21 VARCHAR2(100 BYTE) 
, PIC22 VARCHAR2(100 BYTE) 
, PIC23 VARCHAR2(100 BYTE) 
, PIC24 VARCHAR2(100 BYTE) 
, CONSTRAINT CAR_PK PRIMARY KEY 
  (
    CAR_NUM 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX CAR_PK ON CAR (CAR_NUM ASC) 
      LOGGING 
      TABLESPACE SYSAUX 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        INITIAL 65536 
        NEXT 1048576 
        MINEXTENTS 1 
        MAXEXTENTS UNLIMITED 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE SYSAUX 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  INITIAL 65536 
  NEXT 1048576 
  MINEXTENTS 1 
  MAXEXTENTS UNLIMITED 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;

ALTER TABLE CAR
ADD CONSTRAINT CAR_FK1 FOREIGN KEY
(
  DEALER_ID 
)
REFERENCES MEMBERS
(
  USER_ID 
)
ENABLE;
----
CREATE TABLE BOARD_LIST 
(
  B_CORD NUMBER NOT NULL 
, B_NUM NUMBER NOT NULL 
, USER_NICK VARCHAR2(30 BYTE) NOT NULL 
, B_DATE DATE NOT NULL 
, ADMIN_PERM VARCHAR2(5 BYTE) NOT NULL 
, TITLE VARCHAR2(50 BYTE) NOT NULL 
, USER_ID VARCHAR2(20 BYTE) NOT NULL 
, CONSTRAINT BOARD_LIST_PK PRIMARY KEY 
  (
    B_CORD 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX BOARD_LIST_PK ON BOARD_LIST (B_CORD ASC) 
      LOGGING 
      TABLESPACE SYSAUX 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE SYSAUX 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;

ALTER TABLE BOARD_LIST
ADD CONSTRAINT BOARD_LIST_FK1 FOREIGN KEY
(
  USER_ID 
)
REFERENCES MEMBERS
(
  USER_ID 
)
ENABLE;
