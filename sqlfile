    CREATE TABLE "HR"."CAR" 
   (	"CAR_NUM" VARCHAR2(11 BYTE) NOT NULL ENABLE, 
	"MANUFAC" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"CAR_MODEL" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"CAR_YEAR" NUMBER NOT NULL ENABLE, 
	"CAR_DISTANCE" NUMBER NOT NULL ENABLE, 
	"ACCIDENT_CHEACK" VARCHAR2(9 BYTE) NOT NULL ENABLE, 
	"CAR_KIND" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	"FUEL" VARCHAR2(10 BYTE) NOT NULL ENABLE, 
	"PRICE" NUMBER NOT NULL ENABLE, 
	"PICTURE" VARCHAR2(100 BYTE), 
	"DEALER_ID" VARCHAR2(30 BYTE) NOT NULL ENABLE, 
	"ADMIN_PERM" VARCHAR2(5 BYTE), 
	"UPLOAD_DATE" DATE, 
	 CONSTRAINT "CAR_PK" PRIMARY KEY ("CAR_NUM")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSAUX"  ENABLE, 
	 CONSTRAINT "CAR_FK1" FOREIGN KEY ("DEALER_ID")
	  REFERENCES "HR"."MEMBERS" ("USER_ID") ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSAUX" ;
