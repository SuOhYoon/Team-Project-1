  CREATE TABLE "HR"."BOARD_LIST" 
   (	"B_CORD" NUMBER NOT NULL ENABLE, 
	"B_NUM" NUMBER NOT NULL ENABLE, 
	"USER_NICK" VARCHAR2(30 BYTE) NOT NULL ENABLE, 
	"B_DATE" DATE NOT NULL ENABLE, 
	"ADMIN_PERM" VARCHAR2(5 BYTE) NOT NULL ENABLE, 
	"TITLE" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"USER_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "BOARD_LIST_PK" PRIMARY KEY ("B_CORD")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  TABLESPACE "SYSAUX"  ENABLE, 
	 CONSTRAINT "BOARD_LIST_FK1" FOREIGN KEY ("USER_ID")
	  REFERENCES "HR"."MEMBERS" ("USER_ID") ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  TABLESPACE "SYSAUX" ;
----
  CREATE TABLE "HR"."BOARD" 
   (	"B_CORD" NUMBER NOT NULL ENABLE, 
	"COMMENT_CORD" NUMBER NOT NULL ENABLE, 
	"B_NUM" NUMBER NOT NULL ENABLE, 
	"USER_NICK" VARCHAR2(30 BYTE) NOT NULL ENABLE, 
	"ADMIN_PERM" VARCHAR2(5 BYTE) NOT NULL ENABLE, 
	"B_DATE" VARCHAR2(10 BYTE) NOT NULL ENABLE, 
	"TITLE" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"CONTENT" VARCHAR2(500 BYTE) NOT NULL ENABLE, 
	"USER_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "BOARD_PK" PRIMARY KEY ("COMMENT_CORD")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  TABLESPACE "SYSAUX"  ENABLE, 
	 CONSTRAINT "BOARD_FK1" FOREIGN KEY ("USER_ID")
	  REFERENCES "HR"."MEMBERS" ("USER_ID") ENABLE, 
	 CONSTRAINT "BOARD_FK2" FOREIGN KEY ("B_CORD")
	  REFERENCES "HR"."BOARD_LIST" ("B_CORD") ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  TABLESPACE "SYSAUX" ;
 ----
  CREATE TABLE "HR"."COMMENT_LIST" 
   (	"B_CORD" NUMBER NOT NULL ENABLE, 
	"B_NUM" NUMBER NOT NULL ENABLE, 
	"COMMENT_CORD" NUMBER NOT NULL ENABLE, 
	"USER_NICK" VARCHAR2(30 BYTE) NOT NULL ENABLE, 
	"ADMIN_PERM" VARCHAR2(10 BYTE) NOT NULL ENABLE, 
	"COMMENT_DATE" DATE NOT NULL ENABLE, 
	"COMMENT_CONTENT" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"USER_ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "COMMENT_LIST_FK1" FOREIGN KEY ("USER_ID")
	  REFERENCES "HR"."MEMBERS" ("USER_ID") ENABLE, 
	 CONSTRAINT "COMMENT_LIST_FK2" FOREIGN KEY ("COMMENT_CORD")
	  REFERENCES "HR"."BOARD" ("COMMENT_CORD") ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  TABLESPACE "SYSAUX" ;
